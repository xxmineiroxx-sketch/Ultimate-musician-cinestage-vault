{
  "expo": {
    "name": "Ultimate Playback",
    "slug": "ultimate-playback",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#020617"
    },
    "scheme": "ultimateplayback",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ultimatemusician.playback",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["ultimateplayback"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#020617"
      },
      "package": "com.ultimatemusician.playback",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "ultimateplayback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
